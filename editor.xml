<application>
  <component name="CodeInsightSettings">
    <option name="SHOW_PARAMETER_NAME_HINTS_ON_COMPLETION" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="Returnly" />
  <component name="EditorSettings">
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="KEEP_TRAILING_SPACE_ON_CARET_LINE" value="false" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="#\bTask\b.*">
      <option name="FOREGROUND" value="f9d58" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="3c3f41" />
      <option name="ERROR_STRIPE_COLOR" value="f9d58" />
    </pattern>
    <filter name="All">
      <pattern index="1" />
      <pattern index="0" />
      <pattern index="2" />
    </filter>
    <filter name="Tasks">
      <pattern index="2" />
    </filter>
    <filter name="Todo">
      <pattern index="0" />
    </filter>
  </component>
</application>